{
	"name": "vite-plugin-html-inject-localize",
	"version": "1.0.0",
	"description": "Split your index.html into multiple files and inject them where ever you want! And add localize, if you want.",
	"files": [
		"dist"
	],
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"scripts": {
		"build": "npx unbuild",
		"prepack": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DmitriiNazimov/vite-plugin-html-inject-localize.git"
	},
	"keywords": [
		"vite",
		"vite-plugin",
		"load html",
		"split html",
		"inject html",
		"html",
		"html-loader",
		"loader"
	],
	"author": "Dmitrii Nazimov",
	"license": "MIT",
	"homepage": "https://github.com/DmitriiNazimov/vite-plugin-html-inject-localize",
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^18.0.5",
		"typescript": "^4.7.4",
		"unbuild": "^0.7.4",
		"vite": "^3.0.0"
	}
}
